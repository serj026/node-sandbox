{
  "name": "node-sandbox",
  "version": "0.0.1",
  "description": "Command Processor",
  "author": "Sergey Franchuk",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production npm run build && node build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint --fix",
    "test": "jest --clearCache && jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "generate:proto": "protoc --ts_proto_out=. $(find src -iname \"*.proto\") --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=addNestjsRestParameter=true --ts_proto_opt=esModuleInterop=true --ts_proto_opt=lowerCaseServiceMethods=true"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.4",
    "@grpc/proto-loader": "^0.6.7",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/platform-fastify": "^8.2.3",
    "@nestjs/terminus": "^8.0.3",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "ioredis": "^4.28.2",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "node-rdkafka": "^2.12.0",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@types/bluebird": "^3.5.36",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^26.0.24",
    "@types/jest-when": "^2.7.3",
    "@types/lodash": "^4.14.176",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.11.6",
    "@types/redlock": "^4.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "jest-mock-server": "0.0.5",
    "jest-progress-bar-reporter": "^1.0.21",
    "jest-snapshot": "^27.3.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ts-proto": "^1.95.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2"
  }
}
